{
  "compilerOptions": {
    "target": "ES2024",
    "lib": ["dom", "dom.iterable", "ES2024"],
    "module": "esnext",
    "moduleResolution": "bundler",
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
