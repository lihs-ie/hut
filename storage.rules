rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    match /admin/{allPaths=**} {
      allow read: if true;
      allow write: if request.auth != null;
    }

    match /articles/{allPaths=**} {
      allow read: if true;
      allow write: if request.auth != null;
    }

    match /memos/{allPaths=**} {
      allow read: if true;
      allow write: if request.auth != null;
    }

    match /{allPaths=**} {
      allow read: if true;
      allow write: if false;
    }
  }
}
